<button id="logout" type="button" name="button"><a href="/users/logout">Log out</a></button>
<%= message %>

<ul>
  <% for(var i=0; i<spaces.length; i++) { %>
    <%= spaces[i]['title'] %> - <%= spaces[i]['description'] %> - <%= spaces[i]['price'] %> - <%= spaces[i].users.email %> - <%= spaces[i]['date'] %>

    <button id="reqspace" type="button" name="button">
      <a href= '<%= "/spaces/request/" + spaces[i]["id"]%>'>Request Space</a>
    </button>

    <br>
  <% } %>
</ul>
